{
  "name": "utaba-mcp-shell",
  "version": "1.0.0",
  "description": "MCP Server for controlled command execution in trusted development environments",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "utaba-mcp-shell": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "prepare": "npm run build",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:debug": "vitest --inspect-brk --no-file-parallelism",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "init": "node dist/init.js",
    "release": "npm version patch"
  },
  "keywords": [
    "mcp",
    "shell",
    "command",
    "execution",
    "development",
    "npm",
    "typescript",
    "model-context-protocol",
    "claude",
    "ai"
  ],
  "author": "Development Team",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/utaba-mcp-shell.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/utaba-mcp-shell/issues"
  },
  "homepage": "https://github.com/your-org/utaba-mcp-shell#readme",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "zod": "^3.22.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitest/ui": "^3.1.4",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0",
    "vitest": "^3.1.4"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "config-templates"
  ],
  "engines": {
    "node": ">=18.0.0"
  }
}